{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "complete-application/api/index.ts": {
            "includeFiles": "complete-application/{templates,static}/**"
        },
        "api/index.ts": {
            "includeFiles": "{templates,static}/**"
        }
    }
}