{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/[...all].ts": {
            "includeFiles": "{templates,static}/**"
        },
        "api/index.ts": {
            "includeFiles": "{templates,static}/**"
        }
    }
}